  "version": "1.7.21",
  "dependencies": {
    "js-md4": "^0.3.2",
    "underscore": "~1.7.0"
  },
  "author": {
    "name": "maa105",
    "url": "https://github.com/maa105/"
  },
  "bugs": {
    "url": "https://github.com/SamDecrock/node-http-ntlm/issues"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maa105/node-http-ntlm.git"
  },
  "main": "./build/httpntlm-browserify.js",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
